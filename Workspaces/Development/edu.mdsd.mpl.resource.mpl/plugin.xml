<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="edu.mdsd.mpl.resource.mpl.mopp.MplMetaInformation" id="edu.mdsd.mpl.resource.mpl.mopp.MplMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="edu.mdsd.mpl.resource.mpl.problem">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="edu.mdsd.mpl.resource.mpl.problem">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="edu.mdsd.mpl.resource.mpl.problem">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="edu.mdsd.mpl.resource.mpl.problem">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem.batch_constraint_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="edu.mdsd.mpl.resource.mpl.problem">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem.live_constraint_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="edu.mdsd.mpl.resource.mpl.problem">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="edu.mdsd.mpl.resource.mpl.problem">
		</super>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.nature" name="mpl nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="edu.mdsd.mpl.resource.mpl.mopp.MplNature">
			</run>
		</runtime>
		<builder id="edu.mdsd.mpl.resource.mpl.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="edu.mdsd.mpl.resource.mpl.builder" name="mpl Builder">
		<builder hasNature="true">
			<run class="edu.mdsd.mpl.resource.mpl.mopp.MplBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="edu.mdsd.mpl.resource.mpl.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="edu.mdsd.mpl.resource.mpl.mopp.MplResourceFactoryDelegator" type="mpl">
		</parser>
	</extension>
	
	<extension-point id="edu.mdsd.mpl.resource.mpl.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
	<extension point="org.eclipse.debug.core.breakpoints">
		<breakpoint class="edu.mdsd.mpl.resource.mpl.debug.MplLineBreakpoint" id="edu.mdsd.mpl.resource.mpl.debug.breakpoint" markerType="edu.mdsd.mpl.resource.mpl.debug.lineBreakpoint.marker" name="mpl Breakpoint">
		</breakpoint>
	</extension>
	
	<extension id="edu.mdsd.mpl.resource.mpl.debug.lineBreakpoint.marker" point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.debug.core.lineBreakpointMarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<persistent value="true">
		</persistent>
	</extension>
	
	<extension point="org.eclipse.debug.core.launchConfigurationTypes">
		<launchConfigurationType id="edu.mdsd.mpl.resource.mpl.ui.launchConfigurationType" delegate="edu.mdsd.mpl.resource.mpl.launch.MplLaunchConfigurationDelegate" modes="run,debug" name="mpl Application" sourceLocatorId="edu.mdsd.mpl.resource.mpl.ui.sourceLocator" sourcePathComputerId="edu.mdsd.mpl.resource.mpl.ui.sourcePathComputer" delegateName="mpl Launch Tooling" delegateDescription="This will run or debug .mpl files.">
		</launchConfigurationType>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourcePathComputers">
		<sourcePathComputer class="edu.mdsd.mpl.resource.mpl.debug.MplSourcePathComputerDelegate" id="edu.mdsd.mpl.resource.mpl.ui.sourcePathComputer">
		</sourcePathComputer>
	</extension>
	
	<extension point="org.eclipse.debug.core.sourceLocators">
		<sourceLocator id="edu.mdsd.mpl.resource.mpl.ui.sourceLocator" class="edu.mdsd.mpl.resource.mpl.debug.MplSourceLocator" name="mpl Source Locator">
		</sourceLocator>
	</extension>
	
</plugin>
